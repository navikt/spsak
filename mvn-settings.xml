<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <activeProfiles>
        <activeProfile>repositories</activeProfile>
    </activeProfiles>

    <mirrors>
        <mirror>
            <id>nav-nexus</id>
            <mirrorOf>!public-snapshots,!plugin-snapshots</mirrorOf>
			<!--<url>https://repo.maven.apache.org/maven2/</url>-->
            <url>https://repo.adeo.no/repository/maven-public/</url>
        </mirror>	
    </mirrors>
    
    <profiles>
    
        <profile>
            <id>repositories</id>
            <repositories>
                <repository>
                    <id>public-snapshots</id>
                    <name>Public snapshots</name>
                    <url>https://repo.adeo.no/repository/maven-snapshots/</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                        <updatePolicy>interval:20</updatePolicy>
                    </snapshots>
                </repository>
				<repository>
                    <id>nav-nexus</id>
                    <name>Public snapshots</name>
                    <url>https://repo.adeo.no/repository/maven-public/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>				
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>plugin-snapshots</id>
                    <name>Plugin snapshots</name>
                    <url>https://repo.adeo.no/repository/maven-snapshots/</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                        <updatePolicy>interval:20</updatePolicy>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>			
        </profile>


    </profiles>
 </settings>

