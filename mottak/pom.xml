        <connection>scm:git:ssh://git@stash.devillo.no:7999/vedfp/vl-fordel.git</connection>
        <developerConnection>scm:git:ssh://git@stash.devillo.no:7999/vedfp/vl-fordel.git
        </developerConnection>
        <url>http://stash.devillo.no/projects/VEDFP/repos/vl-fordel/browse</url>
    </scm>

