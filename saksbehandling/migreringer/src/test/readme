
##
## docker run -p 5432:5432 --name some-postgres -e POSTGRES_PASSWORD=mysecretpassword -d postgres:10
##

--Denne må kjøres før unittester:

CREATE DATABASE fpsak_unit;

CREATE USER fpsak_unit PASSWORD 'fpsak_unit';

GRANT ALL PRIVILEGES ON DATABASE fpsak_unit TO fpsak_unit;



-- og denne før spsak startes:

CREATE DATABASE fpsak;

CREATE USER fpsak PASSWORD 'fpsak'; -- annet passord hvis ikke lokalt da

GRANT ALL PRIVILEGES ON DATABASE fpsak TO fpsak;
