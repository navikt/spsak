@Library('deploy')
import deploy

def deployLib = new deploy()

def call(body) {
    try {
		env.LANG = "nb_NO.UTF-8"
		 
        stage ('Build') {
		
			withMaven(globalMavenSettingsConfig: "$mavenConfig", jdk: "$JDKVersion", maven: "$mavenLocation") {
				sh 'mvn -s ../mvn-settings.xml install -DskipTests' 
			}
        }
		
	} catch (error) {
		throw error
	}
}

return this;